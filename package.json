{
  "name": "compliante-solutions-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run validate:images && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "postbuild": "next-sitemap",
    "test": "playwright test",
    "typecheck": "tsc --noEmit",
    "validate:images": "node scripts/validate-images.mjs",
    "check:links": "linkinator http://localhost:3000 --recurse --skip '.*(tel:|mailto:).*'"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@playwright/test": "^1.56.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "linkinator": "^7.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
